	@font-face {
		font-family: 'omega sans'
		src: url('~@/styles/fonts/Omega+Sans+Regular.ttf') format('truetype')
	}

	@font-face {
		font-family: 'kaiju'
		font-style: normal;
  		font-weight: 700;
		src: url('~@/styles/fonts/Kaiju Bold.ttf') format('truetype')
	}

	@font-face {
		font-family: 'kaiju'
		font-style: normal;
  		font-weight: 400;
		src: url('~@/styles/fonts/Kaiju.ttf') format('truetype')
	}

	

