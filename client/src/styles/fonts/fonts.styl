	@font-face {
		font-family: 'Vastine';
		src: local('Vastine'), url('~@/styles/fonts/VastinePersonalUse-jE4ZG.otf') format('opentype');
	}

	@font-face {
		font-family: 'Avenir next'
		src: local('Avenir next'), url('~@/styles/fonts/Nunito-Regular.ttf') format('truetype')
	}

	@font-face {
		font-family: 'searocks'
		src: local('searocks'), url('~@/styles/fonts/Searocks OTF.otf') format('opentype')
	}

	

